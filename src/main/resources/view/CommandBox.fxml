<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>

<StackPane styleClass="command-stack-pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <padding>
    <Insets top="10" right="10" bottom="10" left="10" />
  </padding>
  <TextField fx:id="commandTextField" id="commandTextField" onAction="#handleCommandEntered" promptText="Enter command here..."/>
</StackPane>

